{
  "name": "webpack-dev-server",
  "version": "3.11.0",
  "description": "Serves a webpack app. Updates the browser on changes.",
  "bin": "bin/webpack-dev-server.js",
  "main": "lib/Server.js",
  "files": [
    "bin",
    "lib",
    "ssl",
    "client"
  ],
  "engines": {
    "node": ">= 10.13.0"
  },
  "scripts": {
    "lint:prettier": "prettier \"{**/*,*}.{js,json,md,yml,css}\" --list-different",
    "lint:js": "eslint . --cache",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "lint:type": "tsc --noEmit",
    "commitlint": "commitlint --from=master",
    "security": "npm audit",
    "test:only": "jest --forceExit",
    "test:coverage": "npm run test:only -- --coverage",
    "test:watch": "npm run test:coverage --watch",
    "test": "npm run test:coverage",
    "pretest": "npm run lint",
    "prepare": "rimraf ./ssl/*.pem && npm run build:client",
    "build:client:default": "babel client-src/default --out-dir client/default --ignore \"./client-src/default/*.config.js\"",
    "build:client:clients": "babel client-src/clients --out-dir client/clients",
    "build:client:index": "webpack --color --config client-src/default/webpack.config.js",
    "build:client:sockjs": "webpack --color --config client-src/sockjs/webpack.config.js",
    "build:client": "rimraf ./client/* && npm-run-all -s -l -p \"build:client:**\"",
    "webpack-dev-server": "node examples/run-example.js",
    "release": "standard-version"
  },
  "dependencies": {
    "ansi-html": "0.0.7",
    "bonjour": "^3.5.0",
    "chokidar": "^3.4.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "debug": "^4.1.1",
    "del": "^5.1.0",
    "express": "^4.17.1",
    "html-entities": "^1.3.1",
    "http-proxy-middleware": "^1.0.3",
    "import-local": "^3.0.2",
    "internal-ip": "^6.0.0",
    "ip": "^1.1.5",
    "is-absolute-url": "^3.0.3",
    "killable": "^1.0.1",
    "loglevel": "^1.6.8",
    "open": "^7.0.3",
    "p-retry": "^4.2.0",
    "portfinder": "^1.0.26",
    "schema-utils": "^2.6.6",
    "selfsigned": "^1.10.7",
    "serve-index": "^1.9.1",
    "sockjs": "0.3.20",
    "sockjs-client": "1.4.0",
    "spdy": "^4.0.2",
    "strip-ansi": "^6.0.0",
    "supports-color": "^7.1.0",
    "url": "^0.11.0",
    "webpack-dev-middleware": "^3.7.2",
    "ws": "^7.2.5",
    "yargs": "^13.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@jest/test-sequencer": "^26.0.1",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "commitlint-azure-pipelines-cli": "^1.0.3",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.20.2",
    "execa": "^4.0.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "lint-staged": "^10.2.2",
    "marked": "^1.0.0",
    "memfs": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "puppeteer": "^3.0.4",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.0",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "tcp-port-used": "^1.0.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "webpack-cli": {
      "optional": true
    }
  },
  "author": "Tobias Koppers @sokra",
  "bugs": "https://github.com/webpack/webpack-dev-server/issues",
  "homepage": "https://github.com/webpack/webpack-dev-server#readme",
  "repository": "https://github.com/webpack/webpack-dev-server.git",
  "license": "MIT"
}
