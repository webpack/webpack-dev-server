    "opn": "4.0.2",
    "serve-index": "^1.7.2",
    "sockjs": "0.3.17",
    "sockjs-client": "1.1.1",
    "stream-cache": "~0.0.1",
    "strip-ansi": "^3.0.0",
    "supports-color": "^3.1.1",
    "webpack-dev-middleware": "^1.4.0",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "css-loader": "~0.25.0",
    "eslint": "^3.4.0",
    "file-loader": "~0.9.0",
    "jquery": "^2.2.0",
    "less": "^2.5.1",
    "less-loader": "~2.2.0",
    "pug": "^2.0.0-beta5",
    "pug-loader": "^2.3.0",
    "style-loader": "~0.13.0",
