    "webpack": "^2.1.0-beta.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/webpack/webpack-dev-server.git"
  },
  "homepage": "http://github.com/webpack/webpack-dev-server",
  "main": "lib/Server.js",
  "bin": "bin/webpack-dev-server.js",
  "files": [
    "lib/",
    "bin",
    "client/",
    "ssl/"
